<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net7.0-windows</TargetFramework>
		<UseWPF>true</UseWPF>
		<ApplicationIcon>main.ico</ApplicationIcon>
		<AssemblyVersion>0.8.6.0</AssemblyVersion>
		<FileVersion>0.8.6.0</FileVersion>
		<Version>0.8.6</Version>
		<Authors></Authors>
		<Company>Microsoft Corporation</Company>
		<Product>IR Explorer</Product>
		<ApplicationManifest>app.manifest</ApplicationManifest>
		<AssemblyName>irexplorer</AssemblyName>
		<Platforms>AnyCPU;ARM64</Platforms>
		<LangVersion>default</LangVersion>
	</PropertyGroup>

	<PropertyGroup>
		<ServerGarbageCollection>true</ServerGarbageCollection>
		<ConcurrentGarbageCollection>true</ConcurrentGarbageCollection>
		<GCHeapCount>16</GCHeapCount>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<OutputPath></OutputPath>
		<PlatformTarget>x64</PlatformTarget>
		<DebugType>full</DebugType>
		<DebugSymbols>true</DebugSymbols>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|ARM64'">
		<OutputPath/>
		<PlatformTarget>ARM64</PlatformTarget>
		<DebugType>full</DebugType>
		<DebugSymbols>true</DebugSymbols>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<OutputPath></OutputPath>
		<PlatformTarget>x64</PlatformTarget>
		<DefineConstants>TRACE</DefineConstants>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|ARM64'">
		<OutputPath/>
		<PlatformTarget>ARM64</PlatformTarget>
		<DefineConstants>TRACE</DefineConstants>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>

	<PropertyGroup>
		<Copyright>Microsoft Corporation</Copyright>
		<StartupObject>IRExplorerUI.App</StartupObject>
		<RunPostBuildEvent>OnOutputUpdated</RunPostBuildEvent>
		<RunAnalyzersDuringBuild>false</RunAnalyzersDuringBuild>
		<SignAssembly>false</SignAssembly>
		<RunAnalyzersDuringLiveAnalysis>False</RunAnalyzersDuringLiveAnalysis>
		<AnalysisLevel>none</AnalysisLevel>
		<PlatformTarget>AnyCPU</PlatformTarget>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="Themes\**"/>
		<EmbeddedResource Remove="Themes\**"/>
		<None Remove="Themes\**"/>
		<Page Remove="Themes\**"/>
		<Page Update="Profile\CallTreeNodePanel.xaml">
			<Generator>MSBuild:Compile</Generator>
			<XamlRuntime>Wpf</XamlRuntime>
			<SubType>Designer</SubType>
		</Page>
		<Page Update="Controls\WebViewPopup.xaml">
			<Generator>MSBuild:Compile</Generator>
			<XamlRuntime>Wpf</XamlRuntime>
			<SubType>Designer</SubType>
		</Page>
	</ItemGroup>

	<ItemGroup>
		<None Remove="refresh.png"/>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Autoupdater.NET.Official" Version="1.6.4"/>
		<PackageReference Include="AvalonEdit" Version="6.3.0.90"/>
		<PackageReference Include="Azure.Storage.Blobs" Version="12.11.0"/>
		<PackageReference Include="ClosedXML" Version="0.96.0"/>
		<PackageReference Include="CS-Script.Core" Version="1.4.0"/>
		<PackageReference Include="DiffPlex" Version="1.7.1"/>
		<PackageReference Include="Dirkster.AvalonDock" Version="4.72.1"/>
		<PackageReference Include="Dirkster.AvalonDock.Themes.Aero" Version="4.72.1"/>
		<PackageReference Include="Dirkster.AvalonDock.Themes.Metro" Version="4.72.1"/>
		<PackageReference Include="Dirkster.AvalonDock.Themes.VS2010" Version="4.72.1"/>
		<PackageReference Include="Dirkster.AvalonDock.Themes.VS2013" Version="4.72.1"/>
		<PackageReference Include="DotNetProjects.Extended.Wpf.Toolkit" Version="5.0.100"/>
