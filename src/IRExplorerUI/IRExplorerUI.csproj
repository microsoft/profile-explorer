			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Microsoft.Web.WebView2" Version="1.0.2194-prerelease"/>
		<PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.39"/>
		<PackageReference Include="OxyPlot.SkiaSharp.Wpf" Version="2.1.2"/>
		<PackageReference Include="OxyPlot.Wpf" Version="2.1.2"/>
		<PackageReference Include="protobuf-net" Version="3.0.101"/>
		<PackageReference Include="RangeTree" Version="3.0.1"/>
		<PackageReference Include="System.IO" Version="4.3.0"/>
		<PackageReference Include="HtmlAgilityPack" Version="1.11.42"/>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\ClientServer\GrpcLib.csproj"/>
		<ProjectReference Include="..\external\TreeListView\TreeListView.csproj"/>
		<ProjectReference Include="..\IRExplorerCore\IRExplorerCore.csproj"/>
	</ItemGroup>

	<ItemGroup>
		<Reference Include="Dia2Lib">
			<HintPath>..\external\Dia2Lib.dll</HintPath>
		</Reference>
	</ItemGroup>

	<ItemGroup>
